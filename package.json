{
  "name": "usesthis",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build:start": "build:server && node ./bin/www",
    "bundle": "NODE_ENV=production webpack -p",
    "dockerize": "docker build -t mikewilliamson/usesthis .",
    "test": "jest --runInBand",
    "dump": "arangodump --server.database usesthis_development --output-directory arango_dump_system --overwrite true --include-system-collections true",
    "load": "arangorestore --server.endpoint 'tcp://db.usesth.is:8529' --server.database usesthis_production --input-directory arango_dump_system --overwrite true --include-system-collections true",
    "todos": "grep --color -rin --exclude-dir=node_modules --exclude={bundle.js,*.map} TODO public/",
    "bugs": "grep --color -rin --exclude-dir=node_modules --exclude={bundle.js,*.map} XXX public/",
    "build:server": "babel --copy-files --out-dir dist/ src/"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "react-hot-loader/babel"
    ]
  },
  "dependencies": {
    "@turf/area": "^4.3.0",
    "@turf/bbox": "^4.3.0",
    "@turf/bbox-polygon": "^4.3.0",
    "@turf/explode": "^4.3.0",
    "@turf/helpers": "^4.3.0",
    "@turf/inside": "^4.3.0",
    "@turf/within": "^4.3.0",
    "arangojs": "^5.6.1",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "body-parser": "~1.17.1",
    "color": "^1.0.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.6",
    "express": "~4.15.2",
    "express-graphql": "^0.6.4",
    "express-handlebars": "^3.0.0",
    "graphiql": "^0.10.2",
    "graphql": "^0.9.6",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.8.1",
    "prop-types": "^15.5.10",
    "radium": "^0.18.2",
    "serve-favicon": "~2.4.2",
    "supertest": "^3.0.0",
    "turf-extent": "^1.0.4",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@mapbox/mapbox-gl-geocoder": "^2.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "d3": "^4.8.0",
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.6",
    "d3-format": "^1.2.0",
    "d3-scale": "^1.0.5",
    "d3-selection": "^1.0.6",
    "deep-equal": "^1.0.1",
    "es6-promise": "^4.1.0",
    "imports-loader": "^0.7.1",
    "ismobilejs": "^0.4.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.1",
    "json-loader": "^0.5.4",
    "lodash.countby": "^4.6.0",
    "lodash.differenceby": "^4.8.0",
    "lodash.sortby": "^4.7.0",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "mapbox-gl": "^0.37.0",
    "mapbox-gl-flash": "^2.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-faux-dom": "^3.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-responsive": "^1.3.0",
    "react-router": "^2.8.0",
    "react-tap-event-plugin": "^2.0.1",
    "supertest": "^3.0.0",
    "transform-loader": "^0.2.4",
    "urijs": "^1.18.10",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webworkify": "^1.4.0",
    "webworkify-webpack": "^2.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  }
}
